{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "mint",
	"name": "Fyre Bot",
	"colors": {
		"primary": "#1FA9E1"
	},
	"banner": {
		"content": "Fyre Bot is currently in Early Access. Please report any bugs or give any feedback to @thefallenspirit on Discord."
	},
	"navigation": {
		"dropdowns": [
			{
				"icon": "discord",
				"dropdown": "Fyre Bot",
				"description": "An advanced Discord bot built for developers and power users.",
				"global": {
					"anchors": [
						{
							"icon": "discord",
							"anchor": "Invite / Install",
							"href": "https://discord.com/oauth2/authorize?client_id=1350451983022166158"
						}
					]
				},
				"groups": [
					{
						"group": "The Basics",
						"pages": ["bot/perms/bot"]
					},
					{
						"group": "Miscellaneous",
						"pages": ["bot/misc/variables"]
					}
				]
			}
		]
	},
	"favicon": "/favicon.png",
	"appearance": {
		"strict": true
	},
	"logo": {
		"dark": "/icon.png",
		"light": "/icon.png"
	},
	"footer": {
		"socials": {
			"github": "https://github.com/FyreCorp"
		}
	}
}
